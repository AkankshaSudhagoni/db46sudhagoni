      div(class='parkAttr mr-3') park ID is:
      div #{parkid}
    div.row
      div(class='parkAttr mr-3') parklocation is:
      div #{parklocation}
    div.row
      div(class='parkAttr mr-3') parkarea is:
      div #{parkarea}


